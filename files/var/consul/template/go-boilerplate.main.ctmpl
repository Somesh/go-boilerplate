[Server]
  Host = https://some.domain.com
  AppHost = "apk://"
  Port = 9000
  TemplatePath = /var/www/go-boilerplate/public/template/
  Timeout = {{ key "service/go-boilerplate/Server/Timeout" }}
  PingClientTimeout =  {{ key "service/go-boilerplate/Server/PingClientTimeout" }}
  RatingThreshold = 0
 

[Tracer]
  Enabled = true
  Name = "go-boilerplate"
  Port = 8700
  TTL = 3600
  ExcludeRegex = media|

[Redis "go-boilerplate"]
  Endpoint    = {{ key "service/go-boilerplate/Redis/Endpoint" }}
  Timeout     = {{ key "service/go-boilerplate/Redis/Timeout" }}
  Maxidle     = {{ key "service/go-boilerplate/Redis/Maxidle" }}
  MaxActive   = {{ key "service/go-boilerplate/Redis/MaxActive" }}


{{range $key, $pairs := tree "service/go-boilerplate/Database" | byKey}}
[Database "{{$key}}"]
{{range $pair := $pairs}}  {{.Key}}={{.Value}}
{{end}}{{end}}


[NSQ]
{{ range key "service/go-boilerplate/Nsq/ListenAddress" | split "," }}  ListenAddress = {{ . }}
{{end}}
{{ range key "service/go-boilerplate/Nsq/LookUpAddress" | split "," }}  LookUpAddress = {{ . }}
{{end}}
  PublishAddress = {{ key "service/go-boilerplate/Nsq/PublishAddress"}}
  Prefix = {{ key "service/go-boilerplate/Nsq/Prefix"}}

[Slack]
